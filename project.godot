config_version=5

[addons]

instance_dock/scenes=[]
instance_dock/preview_resolution=Vector2i(64, 64)

[application]

config/name="Crawler"
run/main_scene="res://scenes/levels/level_0.tscn"
config/features=PackedStringArray("4.2", "Mobile")
run/max_fps=75
config/icon="res://icon.svg"

[autoload]

g="*res://scenes/global/global.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/InstanceDock/plugin.cfg", "res://addons/LevelEditor/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg", "res://addons/wiggly_appendage_2d/plugin.cfg")

