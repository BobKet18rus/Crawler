"res://scenes/": "blue",
"res://scripts/": "green",
"res://shaders/": "purple",
"res://sprites/": "yellow",
"res://tools/": "pink",
"res://tools/level_editor/scenes/": "blue",
"res://tools/level_editor/scripts/": "green"
}

